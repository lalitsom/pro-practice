//Complete the staircase function below.

   
    static void staircase(int n) {
      for(int i=n;i>0;i--){
          for(int j=1;j<n+1;j++){
              if(i<=j){
                  System.out.print("#");
              }else{
                   System.out.print(" ");
              }
          }
        System.out.print("\n");
      }

    }
